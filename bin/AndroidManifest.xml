<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectmanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/pm"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.projectmanager.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.projectmanager.AddProject"
            android:label="@string/title_activity_add_project" >
        </activity>
        <activity
            android:name="com.example.projectmanager.ProjectDetails"
            android:label="@string/title_activity_project_details" >
        </activity>
        <activity
            android:name="com.example.projectmanager.ChangeProject"
            android:label="@string/title_activity_change_project" >
        </activity>
    </application>

</manifest>